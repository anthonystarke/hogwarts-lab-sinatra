<nav>
  <ul>
    <li><a href="/create-student">Create a new student</a></li>
  </ul>
</nav>

<h2>All Current Students</h2>
  <% @students.each do |student| %>
    <p> Name: <%= student.first_name %> <%= student.second_name %></p>
    <p> House: <%= student.find_house %>
    <p> Age: <%= student.age %>
      <form method="post" action="/delete/<%=student.id%>"">
        <button>Delete this student</button>
      </form>
      <hr/>

<% end %>
